{
  "name": "2d-math-input-extension",
  "displayName": "2D Math Input Extension",
  "description": "VSCode extension for 2D math input similar to Wolfram Notebook, starting with division",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "2d-math-input-extension.insertFraction",
        "title": "Insert Fraction"
      },
      {
        "command": "2d-math-input-extension.insertSummation",
        "title": "Insert Summation"
      }
    ],
    "keybindings": [
      {
        "command": "2d-math-input-extension.insertFraction",
        "key": "ctrl+/",
        "when": "editorTextFocus"
      }
    ],
    "customEditors": [
      {
        "viewType": "mathEditor",
        "displayName": "Math Editor",
        "selector": [
          {
            "filenamePattern": "*.math"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1",
    "@vscode/test-electron": "^2.4.1"
  }
}